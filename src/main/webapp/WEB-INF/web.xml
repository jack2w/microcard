<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>MicroCard</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>WeixinDev</display-name>
    <servlet-name>WeixinDev</servlet-name>
    <servlet-class>com.microcard.servlet.WeixinDev</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WeixinDev</servlet-name>
    <url-pattern>/WeixinDev</url-pattern>
  </servlet-mapping>  
</web-app>
